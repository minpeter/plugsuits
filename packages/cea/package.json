{
  "name": "@ai-sdk-tool/cea",
  "private": true,
  "version": "1.0.0",
  "description": "A code-editing agent using Vercel AI SDK and FriendliAI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "code-editing-agent": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/entrypoints/cli.ts",
    "headless": "bun run src/entrypoints/headless.ts",
    "dev": "bun run --watch src/entrypoints/cli.ts",
    "build": "tsc",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "check": "bunx ultracite check",
    "lint": "bunx ultracite fix"
  },
  "dependencies": {
    "@ai-sdk-tool/harness": "workspace:*",
    "@ai-sdk-tool/parser": "^4.1.1",
    "@ai-sdk/anthropic": "^3.0.46",
    "@ai-sdk/openai-compatible": "^2.0.30",
    "@friendliai/ai-provider": "^1.1.4",
    "@mariozechner/pi-tui": "^0.54.0",
    "@t3-oss/env-core": "^0.13.10",
    "ai": "^6.0.94",
    "diff": "^8.0.3",
    "glob": "^13.0.6",
    "ignore": "^7.0.5",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^3.0.8",
    "@biomejs/biome": "2.4.0",
    "@types/bun": "^1.3.9",
    "@types/diff": "^8.0.0",
    "@types/node": "^25.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "ultracite": "^7.2.3"
  }
}
