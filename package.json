{
  "name": "code-editing-agent",
  "version": "1.0.0",
  "description": "A code-editing agent using Vercel AI SDK and FriendliAI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "code-editing-agent": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/entrypoints/cli.ts",
    "headless": "bun run src/entrypoints/headless.ts",
    "build": "tsc",
    "dev": "bun run --watch src/entrypoints/cli.ts",
    "check": "ultracite check",
    "lint": "ultracite fix",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "keywords": [
    "ai",
    "agent",
    "code-editing",
    "friendliai",
    "vercel-ai-sdk"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "bun@1.3.1",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@ai-sdk-tool/parser": "^3.3.1",
    "@ai-sdk/openai-compatible": "^2.0.13",
    "@friendliai/ai-provider": "^1.0.4",
    "@t3-oss/env-core": "^0.13.10",
    "ai": "^6.0.39",
    "glob": "^13.0.0",
    "ignore": "^7.0.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^3.0.4",
    "@types/bun": "^1.3.6",
    "@types/node": "^25.0.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "ultracite": "^7.0.11"
  }
}
